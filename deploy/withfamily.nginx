server {
    listen 80;
    server_name yourdomain.com www.yourdomain.com;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        alias /home/youruser/withfamily/staticfiles/;
    }

    location /media/ {
        alias /home/youruser/withfamily/media/;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/run/withfamily.sock;
    }
}
